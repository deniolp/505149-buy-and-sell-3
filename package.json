  "devDependencies": {
    "concurrently": "5.2.0",
    "debug": "4.1.1",
    "eslint": "6.8.0",
    "eslint-config-htmlacademy": "0.5.2",
    "intl": "1.2.5",
    "jest": "26.0.1",
    "nodemon": "2.0.2",
    "pino-debug": "1.3.0",
    "pino-pretty": "4.0.0",
